<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/game.css"/>
    <link rel="shortcut icon" type="image/x-icon" href="../img/logo.png">
    <title>Street TAJA Fighter</title>
    <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
</head>
<body>
    <div class="start">
        <span class="count"></span>
    </div>
    <div>
        <button style="width: 50px; height: 50px;" class="btn" id="playbtn" onclick="MusicPlay()"><img class="btnimg" src="../img/play.png"/></button>
        <button style="width: 50px; height: 50px;" class="btn" id="musicbtn" onclick="MusicSelect()"><img src="../img/music.png"/></button>
    </div>
    <div class="box" id="out1">
        <span class="word-display"></span>
    </div>
    <div id="taja">
        <div class="box" id="out2">
            <!-- <input type="text" class="word-input" placeholder=타자치는곳 autofocus> -->
            <input type="text" class="word-input" autofocus>
        </div>
    </div>

    <p class="state"><span class="NowText">1</span><span>/</span><span class="TotalText">4</span></p>
    
    <div class="effect">
        <!-- 나의조명 -->
        <div class="bulb-holder" id="m1">
            <div class="bulb">
                <div class="light"></div>
            </div>
        </div>
        <div class="bulb-holder" id="m2">
            <div class="bulb">
                <div class="light"></div>
            </div>
        </div>
        <div class="bulb-holder" id="m3">
            <div class="bulb">
                <div class="light"></div>
            </div>
        </div>

        <!-- 상대조명 -->
        <div class="bulb-holder" id="y1">
            <div class="bulb">
                <div class="light"></div>
            </div>
        </div>
        <div class="bulb-holder" id="y2">
            <div class="bulb">
                <div class="light"></div>
            </div>
        </div>
        <div class="bulb-holder" id="y3">
            <div class="bulb">
                <div class="light"></div>
            </div>
        </div>
    </div>

    <div class="effect2">
        <!-- 내꺼 -->
        <img id="m4_1" src="/img/꽃가루효과.gif"/>
        <img id="m4_2" src="/img/꽃가루효과.gif"/>
        <img id="m5" src="/img/꽃가루효과2.gif"/>
        <div class="norespect">

        </div>
        <!-- 상대 -->
        <img id="y4_1" src="/img/꽃가루효과.gif"/>
        <img id="y4_2" src="/img/꽃가루효과.gif"/>
        <img id="y5" src="/img/꽃가루효과2.gif"/>
    </div>
    <div class="Characters">
        <img src="/img/hook/1.png" id="mychar">
        <img src="/img/lachica/1.png" id="youchar">
    </div>
    <button class="ending" onclick="move()"><img src="/img/endingbtn.png"></button>
</body>
<script src = "/js/game.js"></script>
</html>