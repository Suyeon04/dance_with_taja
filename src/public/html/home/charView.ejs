    <img src="/img/textlogo.png" id="Maintitle"/>
    <div>
        <button class="btn" id="playbtn" onclick="MusicPlay()"><img class="btnimg" src="../img/pause.png"/></button>
        <button class="btn" id="musicbtn" onclick="MusicSelect()"><img class="musicimg" src="../img/music.png"/></button>
    </div>
    <div class="content">
        <div class="mychar">
            <div class="Malbum">
                <div class="Mimages">
                    <div class="Mimage"></div>
                    <div class="Mimage"></div>
                    <div class="Mimage"></div>
                    <div class="Mimage"></div>
                </div>
            </div>
            <div class="CharSelect" id="select1">캐릭터 선택 완료!</div>
            <button class="Mprev">PREV</button>
            <button class="Mnext">NEXT</button>
            <h1 style="color: white; text-align: center;">mychar</h1>
            <button class="complete" id="mycomplete" onclick="complete()">COMPLETE</button>
        </div>
        <div class="youchar">
            <div class="Yalbum">
                <div class="Yimages">
                    <div class="Yimage"></div>
                    <div class="Yimage"></div>
                    <div class="Yimage"></div>
                    <div class="Yimage"></div>
                </div>
            </div>
            <div class="CharSelect" id="select2">캐릭터 선택 완료!</div>
            <!-- <button class="Yprev">PREV</button>
            <button class="Ynext">NEXT</button> -->
            <h1 style="color: white; text-align: center;">youchar</h1>
            <button class="complete" id="yourecomplete" onclick="complete2()">COMPLETE</button>
        </div>
    </div> <!--content end-->

